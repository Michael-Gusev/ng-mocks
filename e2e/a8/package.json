    "@types/node": "8.10.66",
    "codelyzer": "5.2.2",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "jest": "25.5.4",
    "jest-preset-angular": "8.2.1",
    "karma": "4.4.1",
